#!/usr/bin/env bash

function validate_encryption_keys {
    set +e

    bundle exec rake db:validate_encryption_keys
    result=$?
    if [[ $result -ne 0 ]]; then
      echo "Encryption key from 'cc.db_encryption_key' is still in use, but no longer present in manifest."
      exit $result
    fi

    set -e
}

function main {
  validate_encryption_keys
}

main

exit 0
